    "bot"
  ],
  "author": "Davide Pedranz & Davide Pizzolotto",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/Pranzio/PranzioBot/issues"
  },
  "homepage": "https://github.com/Pranzio/PranzioBot#readme",
  "private": true,
  "devDependencies": {
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "remap-istanbul": "^0.6.4"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "node-telegram-bot-api": "^0.21.1",
    "redis": "^2.6.0-2",
    "typescript": "^1.8.10"
