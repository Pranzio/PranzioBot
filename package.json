{
  "name": "pranzio_bot",
  "version": "0.1.0",
  "description": "Simple Telegram Bot to fire the \"pranzio\" alarm at launch time.",
  "main": "index.js",
  "scripts": {
    "prestart": "tsc",
    "start": "node src/index.js",
    "pretest": "tsc",
